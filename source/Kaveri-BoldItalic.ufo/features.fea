    # aimatra
    sub ra.base ematra uumatra shortrr' lookup Reph2RaSub;
    # omatra
    sub ra.base ematra uumatra lengthmark shortrr' lookup Reph2RaSub;
    # oomatra
    sub ra.base aumatra shortrr' lookup Reph2RaSub;
    # aumatra
