lookup PreBaseSub {
    lookupflag 0;
    # some consonants have a multiple base forms (.noEbase and .regbase)
    sub @cno_noEbase' @MatraToBase by @c_noEbase;
    sub @cno_regbase' ematra uumatra by @c_regbase;
    # vowel signs AA and AU
    sub @cno_base' @MatraToBase by @c_base;
    # vowel sign and parts with E (but not for the multiple base consonants)
    sub @cno_base' ematra by @c_base;
} PreBaseSub;

lookup PreBaseLig {
    lookupflag 0;
    # custom ligatures
    sub nya umatra by nya_umatra;
    sub lla umatra by lla_umatra;
} PreBaseLig;

lookup LargeEmatraLike {
    lookupflag 0;
    sub fa' aamatra' by fa_aamatra;
    sub nga' aamatra' by nga_aamatra;
    sub nya' aamatra' by nya_aamatra;
    sub rra' aamatra' by rra_aamatra;
    sub fa' ematra' by fa_ematra;
