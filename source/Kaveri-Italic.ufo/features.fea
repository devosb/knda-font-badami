    sub ra ra.below' by ra.below.ra;
    # A ZWNJ has to be explicitly matched
    sub ra zwnj ra.below' by ra.below.ra;
    sub ra.imathra ra.below' by ra.below.ra;
    # Matras that do not have decompositions
    @AtomicMatras = [aamatra imatra umatra uumatra rvocalicmatra rrvocalicmatra ematra aumatra lvocalicmatra llvocalicmatra];
